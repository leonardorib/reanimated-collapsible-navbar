{
  "expo": {
    "name": "reanimated-collapsible-navbar",
    "description": "Collapsible navbar pattern with all animations running on the native thread. Check https://blog.callstack.io/reanimating-your-react-native-experience-d1377d51118a for more details!",
    "slug": "reanimated-collapsible-navbar",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#C2185B"
    }
  }
}
